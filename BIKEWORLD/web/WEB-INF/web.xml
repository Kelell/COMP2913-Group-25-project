<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>Submit</servlet-name>
        <servlet-class>mypack.Submit</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Submit</servlet-name>
        <url-pattern>/submit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Book</servlet-name>
        <servlet-class>mypack.BookABike</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Book</servlet-name>
        <url-pattern>/book</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Booked</servlet-name>
        <servlet-class>mypack.Booked</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Booked</servlet-name>
        <url-pattern>/complete</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>mypack.Registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>mypack.LogIn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/LogIn</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Dash</servlet-name>
        <servlet-class>mypack.Dashboard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dash</servlet-name>
        <url-pattern>/Dashboard</url-pattern>
    </servlet-mapping>
    

    <servlet>
        <servlet-name>Available</servlet-name>
        <servlet-class>mypack.AvailableBikes</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Available</servlet-name>
        <url-pattern>/Available</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>View</servlet-name>
        <servlet-class>mypack.View</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>View</servlet-name>
        <url-pattern>/Views</url-pattern>
    </servlet-mapping>

</web-app>